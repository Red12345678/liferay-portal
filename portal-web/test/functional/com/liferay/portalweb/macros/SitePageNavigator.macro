definition {

	macro gotoAddPage {
		ProductMenuHelper.expandPanel(panel = "Site Administration");

		ProductMenuHelper.expandCategory(category = "Navigation");

		ProductMenuHelper.expandPageTree();

		if ((IsElementNotPresent(locator1 = "ProductMenu#PUBLIC_PAGES")) && (IsElementNotPresent(locator1 = "ProductMenu#PRIVATE_PAGES"))) {
			ProductMenuHelper.addFirstPage(sitePagePrivacy = "${sitePagePrivacy}");
		}
		else {
			if ("${sitePagePrivacy}" == "Private") {
				if (IsElementNotPresent(locator1 = "ProductMenu#PRIVATE_PAGES")) {
					Click(locator1 = "SitePages#PUBLIC_PAGE_ELLIPSIS");

					AssertClick.assertPartialTextClickAt(
						key_menuItem = "Add Private Page",
						locator1 = "MenuItem#ANY_MENU_ITEM",
						value1 = "Add Private Page");
				}
				else if (IsElementNotPresent(locator1 = "ProductMenu#PUBLIC_PAGES")) {
					Click(locator1 = "SitePages#PRIVATE_PAGE_ELLIPSIS");

					AssertClick.assertPartialTextClickAt(
						key_menuItem = "Add Private Page",
						locator1 = "MenuItem#ANY_MENU_ITEM",
						value1 = "Add Private Page");
				}
				else {
					ProductMenu.gotoPrivatePages();

					Click(locator1 = "SitePages#PRIVATE_PAGE_ELLIPSIS");

					AssertClick.assertPartialTextClickAt(
						key_menuItem = "Add Page",
						locator1 = "MenuItem#ANY_MENU_ITEM",
						value1 = "Add Page");
				}
			}
			else {
				ProductMenu.gotoPublicPages();

				Click(locator1 = "SitePages#PUBLIC_PAGE_ELLIPSIS");

				if (IsElementPresent(locator1 = "ProductMenu#PRIVATE_PAGES")) {
					AssertClick.assertPartialTextClickAt(
						key_menuItem = "Add Page",
						locator1 = "MenuItem#ANY_MENU_ITEM",
						value1 = "Add Page");
				}
				else {
					AssertClick.assertPartialTextClickAt(
						key_menuItem = "Add Public Page",
						locator1 = "MenuItem#ANY_MENU_ITEM",
						value1 = "Add Public Page");
				}
			}
		}
	}

	@summary = "Navigate to Product Menu > Sites > Navigation > Public Pages > ${page}"
	macro gotoEditPage {
		ProductMenuHelper.openProductMenu();

		ProductMenuHelper.expandPanel(panel = "Site Administration");

		ProductMenuHelper.expandCategory(category = "Navigation");

		ProductMenuHelper.expandPageTree();

		if ("${sitePagePrivacy}" == "Private Pages") {
			Click(locator1 = "ProductMenu#PRIVATE_PAGES");
		}

		var key_page = "${page}";

		if ("${page}" == "Public Pages") {
			Click(locator1 = "SitePages#PUBLIC_PAGE_ELLIPSIS");

			AssertClick.assertPartialTextClickAt(
				key_menuItem = "Configure",
				locator1 = "MenuItem#ANY_MENU_ITEM",
				value1 = "Configure");
		}
		else if ("${page}" == "Private Pages") {
			Click(locator1 = "SitePages#PRIVATE_PAGE_ELLIPSIS");

			AssertClick.assertPartialTextClickAt(
				key_menuItem = "Configure",
				locator1 = "MenuItem#ANY_MENU_ITEM",
				value1 = "Configure");
		}
		else {
			Click.mouseOverClick(locator1 = "SitePages#SPECIFIC_PAGE_ELLIPSIS");

			AssertClick.assertPartialTextClickAt(
				key_menuItem = "Configure Page",
				locator1 = "MenuItem#ANY_MENU_ITEM",
				value1 = "Configure Page");
		}
	}

	macro gotoPages {
		ProductMenuHelper.openProductMenu();

		ProductMenuHelper.expandPanel(panel = "Site Administration");

		ProductMenuHelper.expandCategory(category = "Navigation");

		ProductMenuHelper.expandPageTree();

		if ("${page}" == "Private Pages") {
			Click(locator1 = "ProductMenu#PRIVATE_PAGES");
		}
	}

}